#!/bin/bash
source /usr/local/Shipper/stock/a52dec_install.sh
if [ "$1" == "order" ];
then
	if [ "$2" == "a52dec" ];
	then
		a52dec_install
	else
		echo -e "\033[91;1;4mError: Product not in stock \033[0m"
		echo "Try refurbishing (updating) VirtualShip."
		fi
if [ "$1" == "refurbish" ];
then
	if [ "$2" == "ship" ];
	then
		./usr/local/Shipper/maintain.sh
	else
		echo -e "\033[91;1;4mError: Product not in stock \033[0m"
		fi
else
	echo -e "\033[91;1;4mError: Command not understood \033[0m"
	fi
fi


